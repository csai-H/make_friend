{"remainingRequest":"D:\\GitRepository\\system-vue-demo\\node_modules\\babel-loader\\lib\\index.js!D:\\GitRepository\\system-vue-demo\\node_modules\\eslint-loader\\index.js??ref--14-0!D:\\GitRepository\\system-vue-demo\\src\\store\\index.js","dependencies":[{"path":"D:\\GitRepository\\system-vue-demo\\src\\store\\index.js","mtime":1713256627459},{"path":"D:\\GitRepository\\system-vue-demo\\babel.config.js","mtime":1713256627449},{"path":"D:\\GitRepository\\system-vue-demo\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1713256876055},{"path":"D:\\GitRepository\\system-vue-demo\\node_modules\\babel-loader\\lib\\index.js","mtime":1713256876371},{"path":"D:\\GitRepository\\system-vue-demo\\node_modules\\eslint-loader\\index.js","mtime":1713256874827}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"names":["Vue","Vuex","persistedState","use","Store","plugins","storage","window","sessionStorage","reducer","state","activeMenu","editableTabsValue","editableTabs","title","name","content","icon","mutations","updateState","value","addTab","item","newTab","meta","path","findIndex","push","removeTab","targetName","tabs","activeName","forEach","tab","index","nextTab","filter","actions","modules"],"sources":["D:/GitRepository/system-vue-demo/src/store/index.js"],"sourcesContent":["/* vuex部分 */\r\nimport Vue from 'vue'\r\nimport Vuex from 'vuex'\r\n/* 引入vuex持久化插件 */\r\nimport persistedState from 'vuex-persistedstate'\r\n\r\nVue.use(Vuex);\r\n\r\nexport default new Vuex.Store({\r\n  /* vuex持久化插件 */\r\n  plugins: [persistedState(\r\n    {\r\n      // 默认位置是 localStorage\r\n      storage: window.sessionStorage,\r\n      reducer: (state) => {\r\n        return {\r\n          // 默认是全部缓存，只储存state中的activeMenu，editableTabsValue以及editableTabs\r\n          activeMenu: state.activeMenu,\r\n          editableTabsValue: state.editableTabsValue,\r\n          editableTabs: state.editableTabs,\r\n        }\r\n      }\r\n    }\r\n  )],\r\n  // 数据，相当于data\r\n  // 使用方法 this.$store.state.全局数据名称\r\n  state: {\r\n    /* 当前激活菜单的 index */\r\n    activeMenu: 'homePage',\r\n    /* 绑定值，选中选项卡的 name */\r\n    editableTabsValue: 'homePage',\r\n    /* tab标签选项卡内容 */\r\n    editableTabs: [\r\n      {\r\n        title: '首页',\r\n        name: 'homePage',\r\n        content: '首页',\r\n        icon: 'el-icon-menu'\r\n      }\r\n    ]\r\n  },\r\n  // 定义方法，用以操作state\r\n  // 注意：使用commit触发Mutation操作\r\n  // 如：this.$store.commit(\"mutations中定义的方法名\",参数)\r\n  mutations: {\r\n    /**\r\n     * 修改state中数据的方法\r\n     * @param state 当前页面的state属性\r\n     * @param name 需要修改的属性名\r\n     * @param value 修改值\r\n     */\r\n    updateState(state, [name, value]) {\r\n      state[name] = value\r\n    },\r\n    /* 动态添加tab标签 */\r\n    addTab(state, item) {\r\n      const newTab = {\r\n        title: item.meta.title,\r\n        name: item.path,\r\n        content: item.meta.title,\r\n        icon: item.meta.icon\r\n      };\r\n      // 判断当前editableTabs中是否存在该tab标签\r\n      if (state.editableTabs.findIndex(item => item.title === newTab.title) === -1) {\r\n        state.editableTabs.push(newTab);\r\n        state.editableTabsValue = newTab.name;\r\n        state.activeMenu = newTab.name;\r\n      }\r\n\r\n    },\r\n    /* 移除tab标签 */\r\n    removeTab(state, targetName) {\r\n      let tabs = state.editableTabs;\r\n      let activeName = state.editableTabsValue;\r\n      if (activeName === targetName) {\r\n        tabs.forEach((tab, index) => {\r\n          if (tab.name === targetName) {\r\n            let nextTab = tabs[index + 1] || tabs[index - 1];\r\n            if (nextTab) {\r\n              activeName = nextTab.name;\r\n            }\r\n          }\r\n        });\r\n      }\r\n      state.activeMenu = activeName;\r\n      state.editableTabsValue = activeName;\r\n      state.editableTabs = tabs.filter(tab => tab.name !== targetName);\r\n    }\r\n  },\r\n  // 定义方法，使用异步操作\r\n  // Action和Mutation相似，Mutation 不能进行异步操作，若要进行异步操作，就得使用Action\r\n  // 直接使用  dispatch触发Action函数 :this.$store.dispatch(\"方法名\",参数)\r\n  actions: {},\r\n  // 当遇见大型项目时，数据量大，store就会显得很臃肿\r\n  // 为了解决以上问题，Vuex 允许我们将 store 分割成模块（module）。\r\n  // 每个模块拥有自己的 state、mutation、action、getter、甚至是嵌套子模块——\r\n  // 从上至下进行同样方式的分割\r\n  modules: {}\r\n})\r\n"],"mappings":";AAAA;AACA,OAAOA,GAAG,MAAM,KAAK;AACrB,OAAOC,IAAI,MAAM,MAAM;AACvB;AACA,OAAOC,cAAc,MAAM,qBAAqB;AAEhDF,GAAG,CAACG,GAAG,CAACF,IAAI,CAAC;AAEb,eAAe,IAAIA,IAAI,CAACG,KAAK,CAAC;EAC5B;EACAC,OAAO,EAAE,CAACH,cAAc,CACtB;IACE;IACAI,OAAO,EAAEC,MAAM,CAACC,cAAc;IAC9BC,OAAO,EAAGC,KAAK,IAAK;MAClB,OAAO;QACL;QACAC,UAAU,EAAED,KAAK,CAACC,UAAU;QAC5BC,iBAAiB,EAAEF,KAAK,CAACE,iBAAiB;QAC1CC,YAAY,EAAEH,KAAK,CAACG;MACtB,CAAC;IACH;EACF,CAAC,CACF,CAAC;EACF;EACA;EACAH,KAAK,EAAE;IACL;IACAC,UAAU,EAAE,UAAU;IACtB;IACAC,iBAAiB,EAAE,UAAU;IAC7B;IACAC,YAAY,EAAE,CACZ;MACEC,KAAK,EAAE,IAAI;MACXC,IAAI,EAAE,UAAU;MAChBC,OAAO,EAAE,IAAI;MACbC,IAAI,EAAE;IACR,CAAC;EAEL,CAAC;EACD;EACA;EACA;EACAC,SAAS,EAAE;IACT;AACJ;AACA;AACA;AACA;AACA;IACIC,WAAW,CAACT,KAAK,EAAE,CAACK,IAAI,EAAEK,KAAK,CAAC,EAAE;MAChCV,KAAK,CAACK,IAAI,CAAC,GAAGK,KAAK;IACrB,CAAC;IACD;IACAC,MAAM,CAACX,KAAK,EAAEY,IAAI,EAAE;MAClB,MAAMC,MAAM,GAAG;QACbT,KAAK,EAAEQ,IAAI,CAACE,IAAI,CAACV,KAAK;QACtBC,IAAI,EAAEO,IAAI,CAACG,IAAI;QACfT,OAAO,EAAEM,IAAI,CAACE,IAAI,CAACV,KAAK;QACxBG,IAAI,EAAEK,IAAI,CAACE,IAAI,CAACP;MAClB,CAAC;MACD;MACA,IAAIP,KAAK,CAACG,YAAY,CAACa,SAAS,CAACJ,IAAI,IAAIA,IAAI,CAACR,KAAK,KAAKS,MAAM,CAACT,KAAK,CAAC,KAAK,CAAC,CAAC,EAAE;QAC5EJ,KAAK,CAACG,YAAY,CAACc,IAAI,CAACJ,MAAM,CAAC;QAC/Bb,KAAK,CAACE,iBAAiB,GAAGW,MAAM,CAACR,IAAI;QACrCL,KAAK,CAACC,UAAU,GAAGY,MAAM,CAACR,IAAI;MAChC;IAEF,CAAC;IACD;IACAa,SAAS,CAAClB,KAAK,EAAEmB,UAAU,EAAE;MAC3B,IAAIC,IAAI,GAAGpB,KAAK,CAACG,YAAY;MAC7B,IAAIkB,UAAU,GAAGrB,KAAK,CAACE,iBAAiB;MACxC,IAAImB,UAAU,KAAKF,UAAU,EAAE;QAC7BC,IAAI,CAACE,OAAO,CAAC,CAACC,GAAG,EAAEC,KAAK,KAAK;UAC3B,IAAID,GAAG,CAAClB,IAAI,KAAKc,UAAU,EAAE;YAC3B,IAAIM,OAAO,GAAGL,IAAI,CAACI,KAAK,GAAG,CAAC,CAAC,IAAIJ,IAAI,CAACI,KAAK,GAAG,CAAC,CAAC;YAChD,IAAIC,OAAO,EAAE;cACXJ,UAAU,GAAGI,OAAO,CAACpB,IAAI;YAC3B;UA